@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix oneM2M: <http://www.owl-ontologies.com/oneM2M#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s4grid: <https://w3id.org/def/saref4grid#> .
@prefix saref: <https://w3id.org/saref#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/def/saref4grid/example/PropertyValue#> a dctype:Dataset ;
  dcterms:license <https://forge.etsi.org/etsi-software-license> ;
  dcterms:conformsTo <https://w3id.org/def/saref4grid#> ;
  dcterms:title "Example Property Value"@en ;
  dcterms:description "Example Property Value"@en .

#################################################################
#    Object Properties
#################################################################

s4grid:hasPropertyValue a owl:ObjectProperty .

saref:hasProperty a owl:ObjectProperty .

saref:hasUnit a owl:ObjectProperty .

skos:broader a owl:ObjectProperty .

skos:narrower a owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

s4grid:hasObis a owl:DatatypeProperty .

s4grid:hasValue a owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

saref:UnitOfMeasure a owl:Class .

s4grid:Meter a owl:Class .

s4grid:PropertyValue a owl:Class .

s4grid:MeterProperty a owl:Class .


#################################################################
#    Individuals
#################################################################

s4grid:Meter1234 a s4grid:Meter,
        owl:NamedIndividual ;
    s4grid:hasPropertyValue s4grid:PropertyValue234,
		s4grid:PropertyValue678.

s4grid:PropertyValue678 a s4grid:PropertyValue,
        owl:NamedIndividual ;
    saref:hasProperty s4grid:MulticastCommunicationIdentifier ;
    saref:hasValue "224.0.1.0"^^xsd:string .

s4grid:ActivePowerLimitContract1TariffPeriod1 a s4grid:MeterProperty,
        owl:NamedIndividual ;
    saref:hasObis "0-1:94.34.1.255"^^xsd:string ;
	skos:broader s4grid:PowerLimit .

s4grid:Network a s4grid:MeterProperty,
        owl:NamedIndividual ;
    skos:narrower s4grid:MulticastCommunicationIdentifier .

om:watt a saref:UnitOfMeasure,
        owl:NamedIndividual ;

s4grid:PropertyValue234 a s4grid:PropertyValue,
        owl:NamedIndividual ;
    saref:hasProperty s4grid:ActivePowerLimitContract1TariffPeriod1 ;
    saref:hasValue "1200"^^xsd:int ;
	saref:hasUnit om:watt .
	
s4grid:PowerLimit a s4grid:MeterProperty,
		owl:NamedIndividual ;
	skos:narrower s4grid:ActivePowerLimitContract1TariffPeriod1 .

s4grid:MulticastCommunicationIdentifier a s4grid:MeterProperty,
        owl:NamedIndividual ;
    saref:hasObis "0-0:96.1.5.255"^^xsd:string ;
    skos:broader s4grid:Network .
